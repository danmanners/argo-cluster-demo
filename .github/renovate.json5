{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:base"],
  dependencyDashboard: true,
  customManagers: [
    {
      customType: "regex",
      fileMatch: ["kustomization.ya??ml$"],
      matchStrings: [
        "datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)\n*[^/]+://[^/]+/[^/]+/[^/]+/(?<currentValue>[^/]+)/",
      ],
    },
    {
      customType: "regex",
      fileMatch: ["env.sh"],
      datasourceTemplate: "helm",
      // versioningTemplate: "semver",
      // matchStringsStrategy: "combination",
      matchStrings: [
        'HELM_REPO_SOURCE="(?<registryUrl>[^"]+)"\n*[^"]+"(?<depName>[^"]+)"\n*[^"]+"(?<currentValue>[^"]+)',
        // 'datasource=(?<datasource>\\S+)\n*[^"]+"(?<registryUrl>[^"]+)"\n*[^"]+"(?<depName>[^"]+)"\n*[^"]+"(?<currentValue>[^"]+)',
        // 'HELM_APP_VERSION="(?<currentValue>[^"]+)',
        // 'HELM_REPO_SOURCE="(?<registryUrl>[^"]+)',
        // 'HELM_APP_NAME="(?<depName>[^"]+)',
      ],
    },
  ],
}
